= form_for @aircraft_type do |f|
  - if @aircraft_type.errors.any?
    #error_explanation
      %h2= "#{pluralize(@aircraft_type.errors.count, "error")} prohibited this aircraft_type from being saved:"
      %ul
        - @aircraft_type.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :type
    = f.text_field :atype
  .actions
    = f.submit 'Save'
