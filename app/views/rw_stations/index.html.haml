%h1 Список rw_stations

%table
  %tr
    %th Station Zone
    %th Name eng
    %th Name rus
    %th Route
    %th Type Station
    %th
    %th
    %th

  - @rw_stations.each do |rw_station|
    %tr
      %td= rw_station.rw_zone
      %td= rw_station.name_eng
      %td= rw_station.name_rus
      %td= rw_station.rw_route_id
      %td= rw_station.rw_typestation.typestation_name
      %td= link_to 'Открыть', rw_station
      %td= link_to 'Редактировать', edit_rw_station_path(rw_station)
      %td= link_to 'Удалить', rw_station, :confirm => 'Вы уверены?', :method => :delete

%br

= link_to 'Создать Rw station', new_rw_station_path

%br
%div{ :style => 'width: 10%; margin: 0px auto;' }
  = will_paginate(@rw_station, :previous_label => '&#8592;', :next_label => '&#8594;')
