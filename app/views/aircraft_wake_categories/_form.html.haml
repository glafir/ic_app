= form_for @aircraft_wake_category do |f|
  - if @aircraft_wake_category.errors.any?
    #error_explanation
      %h2= "#{pluralize(@aircraft_wake_category.errors.count, "error")} prohibited this aircraft_wake_category from being saved:"
      %ul
        - @aircraft_wake_category.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :category
    = f.text_field :category
  .actions
    = f.submit 'Save'
