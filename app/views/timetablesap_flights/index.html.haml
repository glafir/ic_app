%h1 Список timetablesap_flights

%table
  %tr
    %th Timetableap
    %th Start
    %th End
    %th
    %th
    %th

  - @timetablesap_flights.each do |timetablesap_flight|
    %tr
      %td= timetablesap_flight.timetableap_id
      %td= timetablesap_flight.start
      %td= timetablesap_flight.end
      %td= link_to 'Открыть', timetablesap_flight
      %td= link_to 'Редактировать', edit_timetablesap_flight_path(timetablesap_flight)
      %td= link_to 'Удалить', timetablesap_flight, :confirm => 'Вы уверены?', :method => :delete

%br

= link_to 'Создать Timetablesap flight', new_timetablesap_flight_path

%br
%div{ :style => 'width: 10%; margin: 0px auto;' }
  = will_paginate(@timetablesap_flight, :previous_label => '&#8592;', :next_label => '&#8594;')